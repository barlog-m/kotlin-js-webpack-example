{
	"private": true,
	"name": "kotlin-js-webpack-example",
	"version": "0.0.1",
	"scripts": {
		"start": "webpack-dev-server --progress --watch-poll --config webpack.dev.js",
		"build": "webpack --progress --profile --bail --display-reasons --display-error-details --config webpack.prod.js",
		"test": "jest",
		"clean": "rimraf dist kotlin_build",
		"rm": "rimraf dist kotlin_build node_modules",
		"lint": "eslint src",
		"source-map": "webpack --config webpack.prod.js --devtool='source-map'"
	},
	"devDependencies": {
		"@jetbrains/kotlin-webpack-plugin": "^2.0.3",
		"clean-webpack-plugin": "^2.0.1",
		"css-loader": "^2.1.1",
		"file-loader": "^3.0.1",
		"html-webpack-plugin": "^3.2.0",
		"kotlin": "^1.3.21",
		"mini-css-extract-plugin": "^0.5.0",
		"source-map-loader": "^0.2.4",
		"style-loader": "^0.23.1",
		"webpack": "^4.29.6",
		"webpack-cli": "^3.3.0",
		"webpack-dev-server": "^3.2.1",
		"webpack-merge": "^4.2.1"
	},
	"dependencies": {}
}
